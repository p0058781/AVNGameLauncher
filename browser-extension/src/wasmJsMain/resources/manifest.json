{
  "manifest_version": 3,
  "name": "AVN Launcher Browser Extension",
  "version": "1.0",
  "description": "Add F95 game to AVN Game Launcher directly from browser",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "index.html",
    "default_title": "AVN Game Launcher"
  },
  "permissions": [
    "tabs", "activeTab"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self' 'wasm-unsafe-eval'"
  }
}